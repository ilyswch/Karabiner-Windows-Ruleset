{
    "title": "Karabiner-Windows-Ruleset",
    "author": "ilyswch (https://github.com/ilyswch)",
    "maintainers": [
        "ilyswch"
    ],
    "rules": [
        {
            "description": "Windows(WIN) to Ctrl(OSX)",
            "manipulators": [
                {
                    "from": {
                        "key_code": "left_command",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_control"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Ctrl(WIN) to Cmd(OSX)",
            "manipulators": [
                {
                    "from": {
                        "key_code": "left_control",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_command"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "right_control",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_command"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Printscreen to Cmd+Shift+3",
            "manipulators": [
                {
                    "from": {
                        "key_code": "print_screen"
                    },
                    "to": [
                        {
                            "key_code": "3",
                            "modifiers": [
                                "left_command",
                                "left_shift"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Alt+Printscreen to Cmd+Shift+4",
            "manipulators": [
                {
                    "from": {
                        "key_code": "print_screen",
                        "modifiers": {
                            "mandatory": [
                                "left_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "4",
                            "modifiers": [
                                "left_command",
                                "left_shift"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
